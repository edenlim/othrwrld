<p id="notice"><%= notice %></p>
<%= link_to 'Edit', edit_character_path(@character) %> |
<%= link_to 'Back', characters_path %>


<p>
  <strong>Name:</strong>
  <%= @character.name %>
</p>

<p>
  <strong>Story:</strong>
  <% @character.story.each do |story| %>
  <p><%= story.name %></p>
  <% end %>

</p>

<p>
  <strong>Qualities:</strong>
  <% @qualities.each do |key,value| %>
  	<p><%=key%>: <%=value%></p>
  <% end %>
</p>

<p>
	<strong>Relationships:</strong>
	<% @characters.each do |key,value| %>
		<p><%=key%>: <%= value %></p>
	<% end %>
</p>
<div>
	<%= javascript_pack_tag 'hello_react' %>
	<%= javascript_pack_tag 'd3Graph/test.js' %>
</div>