<div class="contain">
<h1>New Character</h1>

<%= form_with scope: :character, url: characters_path, local: true do |form| %>

	<p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>

    <%= form.submit :class => "submit" %>
<% end %>

<%= link_to 'Back', characters_path %>
</div>

<%= stylesheet_link_tag 'new' %>